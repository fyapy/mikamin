{
  "name": "mikamin",
  "version": "1.0.2",
  "description": "Validate schema",
  "author": "Amir Abdullin",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    "./*": {
      "types": "./build/types/*.d.ts",
      "require": "./build/cjs/*.js",
      "import": "./build/esm/*.js",
      "default": "./build/esb/*.js"
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "test": "jest",
    "build": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "bench": "ts-node-esm ./benchmarks/perf.ts"
  },
  "files": [
    "build"
  ],
  "devDependencies": {
    "@swc/cli": "0.1.57",
    "@swc/core": "1.3.14",
    "@swc/jest": "0.2.23",
    "@types/jest": "29.2.2",
    "@types/node": "18.6.3",
    "jest": "29.2.2",
    "mitata": "0.1.6",
    "rimraf": "3.0.2",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
}
